package br.com.supportcomm.virtualgoodscore.common;

public class VirtualGoodsRuntimeException extends RuntimeException
{
    private static final long serialVersionUID = -552243160207784918L;

    public VirtualGoodsRuntimeException()
    {
    }

    public VirtualGoodsRuntimeException(String message)
    {
        super(message);
    }

    public VirtualGoodsRuntimeException(Throwable cause)
    {
        super(cause);
    }

    public VirtualGoodsRuntimeException(String message, Throwable cause)
    {
        super(message, cause);
    }

}
